do001=sin(2^(24/12)*w*t8)+0.1*sin(2.75*2^(24/12)*w*t8)+sin(5.27*2^(24/12)*w*t8)+0.1*sin(8.71*2^(24/12)*w*t8);  %ド+2オクターブ
re001=sin(2^(25/12)*w*t8)+0.1*sin(2.75*2^(25/12)*w*t8)+sin(5.27*2^(25/12)*w*t8)+0.1*sin(8.71*2^(25/12)*w*t8);  %レ+2オクターブ


%2分音符のオルゴール
do02=sin(2^(12/12)*w*t2)+0.1*sin(2.75*2^(12/12)*w*t2)+sin(5.27*2^(12/12)*w*t2)+0.1*sin(8.71*2^(12/12)*w*t2);  %ド＋1オクターブ
do002=sin(2^(24/12)*w*t2)+0.1*sin(2.75*2^(24/12)*w*t2)+sin(5.27*2^(24/12)*w*t2)+0.1*sin(8.71*2^(24/12)*w*t2);  %ド+2オクターブ

%8分音符の太鼓
f1=100;
rap0=(sin(2^(9/12)*2*pi*f1*t8))-(0.3)*sin(2^(9/12)*6*pi*f1*t8)+(0.2)*sin(2^(9/12)*10*pi*f1*t8)-(0.1)*sin(2^(9/12)*14*pi*f1*t8)+(1/81)*sin(2^(9/12)*18*pi*f1*t8);
rap1=(sin(2^(21/12)*2*pi*f1*t8))-(0.3)*sin(2^(21/12)*6*pi*f1*t8)+(0.2)*sin(2^(21/12)*10*pi*f1*t8)-(0.1)*sin(2^(21/12)*14*pi*f1*t8)+(1/81)*sin(2^(21/12)*18*pi*f1*t8);
mip0=(sin(2^(4/12)*2*pi*f1*t8))-(0.3)*sin(2^(4/12)*6*pi*f1*t8)+(0.2)*sin(2^(4/12)*10*pi*f1*t8)-(0.1)*sin(2^(4/12)*14*pi*f1*t8)+(1/81)*sin(2^(4/12)*18*pi*f1*t8);
mip1=(sin(2^(16/12)*2*pi*f1*t8))-(0.3)*sin(2^(16/12)*6*pi*f1*t8)+(0.2)*sin(2^(16/12)*10*pi*f1*t8)-(0.1)*sin(2^(16/12)*14*pi*f1*t8)+(1/81)*sin(2^(16/12)*18*pi*f1*t8);

%8分音符のギター
f2=150;
