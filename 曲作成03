rag8(1:101)=q.*rag8(1:101);              
rasg8(1:101)=q.*rasg8(1:101);              
sig8(1:101)=q.*sig8(1:101);    

dog83(1:101)=q.*dog83(1:101);              dog183(1:101)=q.*dog183(1:101);              
dosg83(1:101)=q.*dosg83(1:101);            dosg183(1:101)=q.*dosg183(1:101);           
reg83(1:101)=q.*reg83(1:101);              reg183(1:101)=q.*reg183(1:101);            
resg83(1:101)=q.*resg83(1:101);            resg183(1:101)=q.*resg183(1:101);             
mig83(1:101)=q.*mig83(1:101);              mig183(1:101)=q.*mig183(1:101);             
fag83(1:101)=q.*fag83(1:101);              fag183(1:101)=q.*fag183(1:101); 
fasg83(1:101)=q.*fasg83(1:101);            fasg183(1:101)=q.*fasg183(1:101); 
sog83(1:101)=q.*sog83(1:101);              sog183(1:101)=q.*sog183(1:101); 
sosg83(1:101)=q.*sosg83(1:101);            sosg183(1:101)=q.*sosg183(1:101); 
rag83(1:101)=q.*rag83(1:101);              rag183(1:101)=q.*rag183(1:101); 
rasg83(1:101)=q.*rasg83(1:101);            
sig83(1:101)=q.*sig83(1:101);    


% 2分音符ギター
rasgg2(1:101)=q.*rasg2(1:101);              
mig2(1:101)=q.*mig2(1:101);    

%曲作成
% ハ短調-シミラにフラット
x1 = [do001 sos01 so01 sos01 do001 sos01 so01 sos01 si01 sos01 so01 sos01 si01 sos01 so01 sos01 si01 sos01 so01 sos01 si01 sos01 so01 sos01 do001 sos01 so01 sos01 do002];
y1 = [do01 z8 res01 z8 do01 z8 res01 z8 re01 z8 fa01 z8 re01 z8 fa01 z8 si1 z8 re01 z8 fa01 z8 re01 z8 do01 z8 res01 z8 do02];
xy1=x1+y1;

x2 = [do001 sos01 so01 sos01 do001 sos01 so01 sos01 re001 ra01 re001 do001 re001 ra01 re001 do001 re001 do001 ra01 do001 re001 do001 ra01 do001 do001 ra01 so01 ra01 do002];
y2=[do01 z8 res01 z8 do01 z8 res01 z8 do01 z8 re01 z8 fas01 z8 re01 z8 do01 z8 re01 z8 fas01 z8 re01 z8 do01 z8 mi01 z8 do02];
% ハ短調-ファソドにフラット
rap=rap0;
mip=mip0;
zz2=[rap z8 z8 z8 z8 z8 rap z8 rap z8 z8 z8 z8 z8 rap z8 rap z8 z8 z8 mip z8 z8 z8 rap z8 z8 z8 z2];
zz2=zz2/max(abs(zz2))*5;
xyz2=x2+y2+zz2;


% ハ短調-シミラにフラット
x33=[sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8 fasg183 z8 fasg183 z8 fasg183 z8 fasg183 z8];
x33=x33/max(abs(x33))*0.2;
x333=[resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 resg183 dog183 reg183 sig83 reg183 sig83 reg183 sig83 reg183 sig83];
x333=x333/max(abs(x333))*0.99;
x3=x33+x333;
% ハ短調-ファソドにフラット
ga=460832-460824;
za=zeros(1,ga); 
zz3=[rag4 z8 sig4 dog14 mig18 rag4 z8 z8 z8 z8 z8 z8 rag4 z8 sig4 dog14 mig18 resg14 z8 z8 z8 z8 z8 z8 za];
xz3=x3+zz3;

% ハ短調-シミラにフラット
x44=[fasg183 z8 fasg183 z8 fasg183 z8 fasg183 z8 rag183 z8 rag183 z8 rag183 z8 rag183 z8 rag183 z8 rag183 z8 rag183 z8 rag183 z8 sog183 z8 sog183 z8 sog183 z8 sog183 z8];
x44=x44/max(abs(x44))*0.2;
x444=[reg183 sig83 reg183 reg183 reg183 sig83 reg183 reg183 mig183 dog183 mig183 dog183 mig183 dog183 mig183 dog183 fag183 dog183 fag183 dog183 fag183 dog183 fag183 dog183 fag183 reg183 fag183 reg183 fag183 reg183 fag183 reg183];
x444=x444/max(abs(x444))*0.99;
x4=x44+x444;
% ハ短調-ファソドにフラット
gb=460832-460816;
zb=zeros(1,gb); 
zz4=[sog4 z8 rasg8 sig4 sosg4 fasg4 rasg2 fasg4 mig2 rasg4 reg14 mig14 z8 reg18 dog14 sig4 zb];
xz4=x4+zz4;

zc=zeros(1,3000-2000);
A = [xy1 z xyz2 z xz3 zc xz4];
A=A/max(abs(A))*0.99; %最大値を1未満にする
% % zz3=zz3/max(abs(zz3))*0.99; %最大値を1未満にする
% % zz4=zz4/max(abs(zz4))*0.99; %最大値を1未満にする
% % xz3=xz3/max(abs(xz3))*0.99; %最大値を1未満にする
% % xz4=xz4/max(abs(xz4))*0.99; %最大値を1未満にする
playblocking(audioplayer(A,fs))
