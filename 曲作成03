clear ; close all

fs=48000;
x=500;                            %オルゴールにおけるドの基本周波数

N4=110;                     %4分音符が一分間で110個

N=60/100;
t1=0:1/fs:N*4;                      %全音符/休符の長さ
t2=0:1/fs:N*2;                    %2分音符/休符の長さ
t4=0:1/fs:N;                     %4分音符/休符の長さ
t8=0:1/fs:N/2;                    %8分音符/休符の長さ
t16=0:1/fs:N/4;                    %16分音符/休符の長さ
t32=0:1/fs:N/8;                  %32分音符/休符の長さ



w=2*pi*x;
%8分音符のオルゴール
do1=sin(w*t8)+0.1*sin(2.75*w*t8)+sin(5.27*w*t8)+0.1*sin(8.71*w*t8);                                          %ド
dos1=sin(2^(1/12)*w*t8)+0.1*sin(2.75*2^(1/12)*w*t8)+sin(5.27*2^(1/12)*w*t8)+0.1*sin(8.71*2^(1/12)*w*t8);      %ド＃
re1=sin(2^(2/12)*w*t8)+0.1*sin(2.75*2^(2/12)*w*t8)+sin(5.27*2^(2/12)*w*t8)+0.1*sin(8.71*2^(2/12)*w*t8);       %レ
res1=sin(2^(3/12)*w*t8)+0.1*sin(2.75*2^(3/12)*w*t8)+sin(5.27*2^(3/12)*w*t8)+0.1*sin(8.71*2^(3/12)*w*t8);      %レ＃
mi1=sin(2^(4/12)*w*t8)+0.1*sin(2.75*2^(4/12)*w*t8)+sin(5.27*2^(4/12)*w*t8)+0.1*sin(8.71*2^(4/12)*w*t8);       %ミ
fa1=sin(2^(5/12)*w*t8)+0.1*sin(2.75*2^(5/12)*w*t8)+sin(5.27*2^(5/12)*w*t8)+0.1*sin(8.71*2^(5/12)*w*t8);       %ファ
fas1=sin(2^(6/12)*w*t8)+0.1*sin(2.75*2^(6/12)*w*t8)+sin(5.27*2^(6/12)*w*t8)+0.1*sin(8.71*2^(6/12)*w*t8);      %ファ＃
so1=sin(2^(7/12)*w*t8)+0.1*sin(2.75*2^(7/12)*w*t8)+sin(5.27*2^(7/12)*w*t8)+0.1*sin(8.71*2^(7/12)*w*t8);       %ソ
sos1=sin(2^(8/12)*w*t8)+0.1*sin(2.75*2^(8/12)*w*t8)+sin(5.27*2^(8/12)*w*t8)+0.1*sin(8.71*2^(8/12)*w*t8);      %ソ＃
ra1=sin(2^(9/12)*w*t8)+0.1*sin(2.75*2^(9/12)*w*t8)+sin(5.27*2^(9/12)*w*t8)+0.1*sin(8.71*2^(9/12)*w*t8);       %ラ
ras1=sin(2^(10/12)*w*t8)+0.1*sin(2.75*2^(10/12)*w*t8)+sin(5.27*2^(10/12)*w*t8)+0.1*sin(8.71*2^(10/12)*w*t8);  %ラ＃
si1=sin(2^(11/12)*w*t8)+0.1*sin(2.75*2^(11/12)*w*t8)+sin(5.27*2^(11/12)*w*t8)+0.1*sin(8.71*2^(11/12)*w*t8);   %シ
do01=sin(2^(12/12)*w*t8)+0.1*sin(2.75*2^(12/12)*w*t8)+sin(5.27*2^(12/12)*w*t8)+0.1*sin(8.71*2^(12/12)*w*t8);  %ド＋1オクターブ
dos01=sin(2^(13/12)*w*t8)+0.1*sin(2.75*2^(13/12)*w*t8)+sin(5.27*2^(13/12)*w*t8)+0.1*sin(8.71*2^(13/12)*w*t8); %ド#＋1オクターブ
re01=sin(2^(14/12)*w*t8)+0.1*sin(2.75*2^(14/12)*w*t8)+sin(5.27*2^(14/12)*w*t8)+0.1*sin(8.71*2^(14/12)*w*t8);  %レ＋1オクターブ
res01=sin(2^(15/12)*w*t8)+0.1*sin(2.75*2^(15/12)*w*t8)+sin(5.27*2^(15/12)*w*t8)+0.1*sin(8.71*2^(15/12)*w*t8); %レ＃＋1オクターブ
mi01=sin(2^(16/12)*w*t8)+0.1*sin(2.75*2^(16/12)*w*t8)+sin(5.27*2^(16/12)*w*t8)+0.1*sin(8.71*2^(16/12)*w*t8);  %ミ＋1オクターブ
fa01=sin(2^(17/12)*w*t8)+0.1*sin(2.75*2^(17/12)*w*t8)+sin(5.27*2^(17/12)*w*t8)+0.1*sin(8.71*2^(17/12)*w*t8);  %ファ＋1オクターブ
fas01=sin(2^(18/12)*w*t8)+0.1*sin(2.75*2^(18/12)*w*t8)+sin(5.27*2^(18/12)*w*t8)+0.1*sin(8.71*2^(18/12)*w*t8); %ファ＃＋1オクターブ
so01=sin(2^(19/12)*w*t8)+0.1*sin(2.75*2^(19/12)*w*t8)+sin(5.27*2^(19/12)*w*t8)+0.1*sin(8.71*2^(19/12)*w*t8);  %ソ＋1オクターブ
sos01=sin(2^(20/12)*w*t8)+0.1*sin(2.75*2^(20/12)*w*t8)+sin(5.27*2^(20/12)*w*t8)+0.1*sin(8.71*2^(20/12)*w*t8); %ソ＃＋1オクターブ
ra01=sin(2^(21/12)*w*t8)+0.1*sin(2.75*2^(21/12)*w*t8)+sin(5.27*2^(21/12)*w*t8)+0.1*sin(8.71*2^(21/12)*w*t8);  %ラ＋1オクターブ
ras01=sin(2^(22/12)*w*t8)+0.1*sin(2.75*2^(22/12)*w*t8)+sin(5.27*2^(22/12)*w*t8)+0.1*sin(8.71*2^(22/12)*w*t8); %ラ＃＋1オクターブ
