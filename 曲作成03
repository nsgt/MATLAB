resg14=0.316228*sin(2^(15/12)*2*pi*f3*t4)+2.54951*sin(2^(15/12)*2*2*pi*f3*t4)+2.24103*sin(2^(15/12)*3*2*pi*f3*t4)+0.74162*sin(2^(15/12)*4*2*pi*f3*t4)+0.583095*sin(2^(15/12)*5*2*pi*f3*t4)+0.353553*sin(2^(15/12)*6*2*pi*f3*t4)+0.0451754*sin(2^(15/12)*7*2*pi*f3*t4)+0.273861*sin(2^(15/12)*8*2*pi*f3*t4)+0.0351364*sin(2^(15/12)*9*2*pi*f3*t4);
mig14=0.316228*sin(2^(16/12)*2*pi*f3*t4)+2.54951*sin(2^(16/12)*2*2*pi*f3*t4)+2.24103*sin(2^(16/12)*3*2*pi*f3*t4)+0.74162*sin(2^(16/12)*4*2*pi*f3*t4)+0.583095*sin(2^(16/12)*5*2*pi*f3*t4)+0.353553*sin(2^(16/12)*6*2*pi*f3*t4)+0.0451754*sin(2^(16/12)*7*2*pi*f3*t4)+0.273861*sin(2^(16/12)*8*2*pi*f3*t4)+0.0351364*sin(2^(16/12)*9*2*pi*f3*t4);

% 2分音符ギター
rasg2=0.316228*sin(2^(10/12)*2*pi*f3*t2)+2.54951*sin(2^(10/12)*2*2*pi*f3*t2)+2.24103*sin(2^(10/12)*3*2*pi*f3*t2)+0.74162*sin(2^(10/12)*4*2*pi*f3*t2)+0.583095*sin(2^(10/12)*5*2*pi*f3*t2)+0.353553*sin(2^(10/12)*6*2*pi*f3*t2)+0.0451754*sin(2^(10/12)*7*2*pi*f3*t2)+0.273861*sin(2^(10/12)*8*2*pi*f3*t2)+0.0351364*sin(2^(10/12)*9*2*pi*f3*t2);
mig2=0.316228*sin(2^(4/12)*2*pi*f3*t2)+2.54951*sin(2^(4/12)*2*2*pi*f3*t2)+2.24103*sin(2^(4/12)*3*2*pi*f3*t2)+0.74162*sin(2^(4/12)*4*2*pi*f3*t2)+0.583095*sin(2^(4/12)*5*2*pi*f3*t2)+0.353553*sin(2^(4/12)*6*2*pi*f3*t2)+0.0451754*sin(2^(4/12)*7*2*pi*f3*t2)+0.273861*sin(2^(4/12)*8*2*pi*f3*t2)+0.0351364*sin(2^(4/12)*9*2*pi*f3*t2);



%休符
z2=zeros(1,57601);    %2分休符
z4=zeros(1,28801);    %4分休符
z8=zeros(1,14401);    %8分休符

%間
z=zeros(1,3000);

%指数減衰を加えた音符作成
a=-20*log(0.5);             % 0.05秒で0.5倍に減衰 
ee8=exp(-a*t8);              % 指数減衰関数を代入(8分音符)
ee2=exp(-a*t2);              % 指数減衰関数を代入(2分音符)
ee4=exp(-a*t4);              % 指数減衰関数を代入(4分音符)
%8分音符オルゴール
do1=ee8.*do1;               do01=ee8.*do01;              do001=ee8.*do001;
dos1=ee8.*dos1;             dos01=ee8.*dos01;            do02=ee2.*do02;
re1=ee8.*re1;               re01=ee8.*re01;              do002=ee2.*do002;
res1=ee8.*res1;             res01=ee8.*res01;            re001=ee8.*re001;
mi1=ee8.*mi1;               mi01=ee8.*mi01;              
fa1=ee8.*fa1;               fa01=ee8.*fa01;
fas1=ee8.*fas1;             fas01=ee8.*fas01;
so1=ee8.*so1;               so01=ee8.*so01; 
sos1=ee8.*sos1;             sos01=ee8.*sos01;   
ra1=ee8.*ra1;               ra01=ee8.*ra01; 
ras1=ee8.*ras1;             ras01=ee8.*ras01; 
si1=ee8.*si1;               si01=ee8.*si01; 

%8分音符の太鼓
rap0=ee8.*rap0;               rap1=ee8.*rap1;
mip0=ee8.*mip0;              mip1=ee8.*mip1;

b=-5*log(0.5);
ee88=exp(-b*t8);              % 指数減衰関数を代入(8分音符)
ee44=exp(-b*t4);              % 指数減衰関数を代入(4分音符)
ee22=exp(-b*t2);              % 指数減衰関数を代入(2分音符)

%8分音符ギター
dog8=ee88.*dog8;               dog18=ee88.*dog18;              
dosg8=ee88.*dosg8;             dosg18=ee88.*dosg18;            
reg8=ee88.*reg8;               reg18=ee88.*reg18;              
resg8=ee88.*resg8;             resg18=ee88.*resg18;            
mig8=ee88.*mig8;               mig18=ee88.*mig18;              
fag8=ee88.*fag8;               
fasg8=ee88.*fasg8;          
sog8=ee88.*sog8;                
sosg8=ee88.*sosg8;             
rag8=ee88.*rag8;               
rasg8=ee88.*rasg8;             
sig8=ee88.*sig8;               

dog83=ee88.*dog83;               dog183=ee88.*dog183;              
dosg83=ee88.*dosg83;             dosg183=ee88.*dosg183;            
reg83=ee88.*reg83;               reg183=ee88.*reg183;              
resg83=ee88.*resg83;             resg183=ee88.*resg183;            
mig83=ee88.*mig83;               mig183=ee88.*mig183;              
fag83=ee88.*fag83;               fag183=ee88.*fag183; 
fasg83=ee88.*fasg83;             sog183=ee88.*sog183; 
sog83=ee88.*sog83;               sosg183=ee88.*sosg183; 
sosg83=ee88.*sosg83;             rag183=ee88.*rag183; 
rag83=ee88.*rag83;               
rasg83=ee88.*rasg83;             
sig83=ee88.*sig83;               


%4分音符ギター
dog4=ee44.*dog4;               dog14=ee44.*dog14;              
dosg4=ee44.*dosg4;             dosg14=ee44.*dosg14;            
reg4=ee44.*reg4;               reg14=ee44.*reg14;              
resg4=ee44.*resg4;             resg14=ee44.*resg14;            
mig4=ee44.*mig4;               mig14=ee44.*mig14;              
fag4=ee44.*fag4;               
fasg4=ee44.*fasg4;          
sog4=ee44.*sog4;                
sosg4=ee44.*sosg4;             
rag4=ee44.*rag4;               
rasg4=ee44.*rasg4;             
sig4=ee44.*sig4;               

% 2分音符ギター
rasg2=ee22.*rasg2;
mig2=ee22.*mig2;


%立ち上がりの作成
w=0.1/2000:1;   %先頭の4000点に傾きをつける
rap0(1:1001)=w.*rap0(1:1001);     rap1(1:1001)=w.*rap1(1:1001);
mip0(1:1001)=w.*mip0(1:1001);     mip1(1:1001)=w.*mip1(1:1001);

%4分音符ギター
q=0.1/500:1; 
dog4(1:101)=q.*dog4(1:101);              dog14(1:101)=q.*dog14(1:101);              
dosg4(1:101)=q.*dosg4(1:101);            dosg14(1:101)=q.*dosg14(1:101);           
reg4(1:101)=q.*reg4(1:101);              reg4(1:101)=q.*reg14(1:101);            
resg4(1:101)=q.*resg4(1:101);            resg14(1:101)=q.*resg14(1:101);             
mig4(1:101)=q.*mig4(1:101);              mig14(1:101)=q.*mig14(1:101);             
fag4(1:101)=q.*fag4(1:101);               
fasg4(1:101)=q.*fasg4(1:101);          
sog4(1:101)=q.*sog4(1:101);                
sosg4(1:101)=q.*sosg4(1:101);            
rag4(1:101)=q.*rag4(1:101);              
rasg4(1:101)=q.*rasg4(1:101);              
sig4(1:101)=q.*sig4(1:101);    

% 8分音符ギター
dog8(1:101)=q.*dog8(1:101);              dog18(1:101)=q.*dog18(1:101);              
dosg8(1:101)=q.*dosg8(1:101);            dosg18(1:101)=q.*dosg18(1:101);           
reg8(1:101)=q.*reg8(1:101);              reg18(1:101)=q.*reg18(1:101);            
resg8(1:101)=q.*resg8(1:101);            resg18(1:101)=q.*resg18(1:101);             
mig8(1:101)=q.*mig8(1:101);              mig18(1:101)=q.*mig18(1:101);             
fag8(1:101)=q.*fag8(1:101);               
fasg8(1:101)=q.*fasg8(1:101);          
sog8(1:101)=q.*sog8(1:101);                
