clear all; close all
%①
fs=8000;
x=261.6; %ドの周波数
t=[1:fs/2]/fs;
do=sin(2*pi*x*t);
re=sin(2*pi* x*2^(2/12) *t);
mi=sin(2*pi* x*2^(4/12) *t);
fa=sin(2*pi* x*2^(6/12) *t);
so=sin(2*pi* x*2^(8/12) *t);
ra=sin(2*pi* x*2^(10/12) *t);
si=sin(2*pi* x*2^(12/12) *t);
re=sin(2*pi* x*2^(1/12) *t);





wavplay(do,fs)
wavplay(re,fs)
